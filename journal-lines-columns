--- for spare1 and spare2 
SELECT 
    JL.SEGMENT1 AS Spare1_Segment,
    JL.SEGMENT2 AS Spare2_Segment
FROM 
    GL_JE_LINES JL
JOIN 
    GL_JE_HEADERS JH ON JL.JE_HEADER_ID = JH.JE_HEADER_ID
WHERE 
    JL.LEDGER_ID = :P_LEDGER_ID
    AND JH.PERIOD_NAME = :P_PERIOD_NAME
    AND JH.CURRENCY_CODE = :P_CURRENCY_CODE